{
  "expo": {
    "name": "lumiere",
    "slug": "lumiere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2EFE9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bot.lumiere.app",
      "buildNumber": "1",
      "requiresFullScreen": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Lumiere needs access to your microphone to transcribe voice messages.",
        "NSSpeechRecognitionUsageDescription": "Lumiere uses speech recognition to convert your voice to text.",
        "NSCalendarsUsageDescription": "Lumiere needs access to your calendar to create and store events.",
        "UIRequiresFullScreen": false,
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "lumiere"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "bot.lumiere.app"
    },
    "platforms": ["ios", "android", "macos"],
    "scheme": "lumiere",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#F2EFE9",
          "dark": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-quick-actions",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1",
            "catalyst": {
              "enabled": true
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "24d55021-5419-4ad2-b491-7aad43cbbdec"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/24d55021-5419-4ad2-b491-7aad43cbbdec"
    }
  }
}
